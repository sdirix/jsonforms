{
	"name": "@jsonforms/vanilla-renderers",
	"version": "3.0.0-beta.5",
	"description": "Default Renderer Set for JSON Forms",
	"repository": "https://github.com/eclipsesource/jsonforms",
	"bugs": "https://github.com/eclipsesource/jsonforms/issues",
	"homepage": "http://jsonforms.io/",
	"license": "MIT",
	"main": "lib/jsonforms-react-vanilla.cjs.js",
	"module": "lib/jsonforms-react-vanilla.esm.js",
	"typings": "lib/index.d.ts",
	"keywords": [
		"vanilla",
		"form",
		"forms",
		"json",
		"jsonforms",
		"frontend",
		"generator",
		"input",
		"renderengine",
		"jsonschema",
		"schema",
		"uischema",
		"layout",
		"customization"
	],
	"peerDependencies": {
		"@jsonforms/core": "3.0.0-beta.5",
		"@jsonforms/react": "3.0.0-beta.5",
		"react": "^16.12.0 || ^17.0.0 || ^18.0.0"
	},
	"devDependencies": {
		"typedoc": "^0.23.9"
	},
	"jest": {
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js"
		],
		"transform": {
			"^.+\\.(ts|tsx)$": "ts-jest"
		},
		"testMatch": [
			"**/test/**/*.test.tsx"
		],
		"testPathIgnorePatterns": [
			"/node_modules/",
			"/dist/"
		],
		"globals": {
			"ts-jest": {
				"tsconfig": "tsconfig.test.json"
			}
		}
	},
	"nyc": {
		"reporter": [
			"lcov",
			"text-summary"
		]
	}
}
